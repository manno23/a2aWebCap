  },
  "dependencies": {
    "@a2a-webcap/shared": "workspace:*",
    "capnweb": "^0.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251117.0",
    "@types/node": "^20.19.25",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "wrangler": "^4.47.0"
  }
}
